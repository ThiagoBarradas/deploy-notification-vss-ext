{
    "manifestVersion": 1,
    "id": "deploy-notification",
    "publisher": "ThiagoBarradas",
    "version": "1.4.0",
    "name": "Deploy Notification",
    "description": "Send notification with new version and rollback version to Slack",
    "public": true,
    "links": {
        "home": { "uri": "https://github.com/ThiagoBarradas/deploy-notification-vss-ext#readme" },
        "repository": { "uri": "https://github.com/ThiagoBarradas/deploy-notification-vss-ext" },
        "support": { "uri": "https://github.com/ThiagoBarradas/deploy-notification-vss-ext/issues" },
        "getstarted": { "uri": "https://github.com/ThiagoBarradas/deploy-notification-vss-ext#readme" },
        "privacypolicy": { "uri" : "https://gist.github.com/ThiagoBarradas/eb56552b4a71cf52141b85a6afa0e526" }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "notification",
        "deploy",
        "rollback",
        "notification",
        "slack",
        "release"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "targets": [
        { "id": "Microsoft.VisualStudio.Services" }
    ],
    "content": {
        "details": { "path": "README.md" },
        "license": { "path": "LICENSE" }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/ThiagoBarradas/deploy-notification-vss-ext"
    },
    "contributions": [
        {
            "id": "deploy-notification",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": { "name": "task" }
        }
    ],
    "files": [
        { 
            "path": "task" 
        },
        { 
            "path": "images", 
            "addressable": true 
        },
        { 
            "path": "node_modules", 
            "packagePath": "task/node_modules" 
        }
    ]
}